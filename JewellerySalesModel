package com.ntj.model;

public class JewellerySalesModel {

	String salesBillNo,salesBillDate,salesBillMobile,salesBillName,salesBillDiscount,salesBillTotItems,salesBillTotWeight,salesBillMode,salesBillTime;
	String salesItemNo,salesItemCode,salesItemType,salesItemCount,salesItemWeight,salesItemPurity,salesItemWastage,salesItemPure,salesItemMode,salesItemTime;
	String salesMetalMno,salesMetalWeight,salesMetalResult,salesMetalPure,salesMetalMode,salesMetalTime;
	String salesBillDateFrom,salesBillDateTo;

public JewellerySalesModel() {
		super(); 
		}

public JewellerySalesModel(String salesBillNo, String salesBillDate, String salesBillMobile, String salesBillName,
			String salesBillDiscount, String salesBillTotItems, String salesBillTotWeight, String salesBillMode,
			String salesBillTime, String salesItemNo, String salesItemCode, String salesItemType,String salesItemCount,
			String salesItemWeight, String salesItemPurity, String salesItemWastage, String salesItemPure,
			String salesItemMode, String salesItemTime,String salesMetalMno, String salesMetalWeight, String salesMetalResult,
			String salesMetalPure,String salesMetalMode,String salesMetalTime) {
		super();
		this.salesBillNo = salesBillNo;
		this.salesBillDate = salesBillDate;
		this.salesBillMobile = salesBillMobile;
		this.salesBillName = salesBillName;
		this.salesBillDiscount = salesBillDiscount;
		this.salesBillTotItems = salesBillTotItems;
		this.salesBillTotWeight = salesBillTotWeight;
		this.salesBillMode = salesBillMode;
		this.salesBillTime = salesBillTime;
		this.salesItemNo = salesItemNo;
		this.salesItemCode = salesItemCode;
		this.salesItemType = salesItemType;
		this.salesItemCount = salesItemCount;
		this.salesItemWeight = salesItemWeight;
		this.salesItemPurity = salesItemPurity;
		this.salesItemWastage = salesItemWastage;
		this.salesItemPure = salesItemPure;
		this.salesItemMode = salesItemMode;
		this.salesItemTime = salesItemTime;
		this.salesMetalMno = salesMetalMno;
		this.salesMetalWeight = salesMetalWeight;
		this.salesMetalResult = salesMetalResult;
		this.salesMetalPure = salesMetalPure;
		this.salesMetalMode = salesMetalMode;
		this.salesMetalTime = salesMetalTime;
		
	}

public JewellerySalesModel(String salesBillNo, String salesBillDate, String salesBillMobile, String salesBillName,
			String salesBillDiscount, String salesBillTotItems, String salesBillTotWeight, String salesBillMode,
			String salesBillTime) {
		super();
		this.salesBillNo = salesBillNo;
		this.salesBillDate = salesBillDate;
		this.salesBillMobile = salesBillMobile;
		this.salesBillName = salesBillName;
		this.salesBillDiscount = salesBillDiscount;
		this.salesBillTotItems = salesBillTotItems;
		this.salesBillTotWeight = salesBillTotWeight;
		this.salesBillMode = salesBillMode;
		this.salesBillTime = salesBillTime;
	}

public JewellerySalesModel(String salesBillNo, String salesItemNo, String salesItemCode, String salesItemType,String salesItemCount,
			String salesItemWeight, String salesItemPurity, String salesItemWastage, String salesItemPure,
			String salesItemMode, String salesItemTime) {
		super();
		this.salesBillNo = salesBillNo;
		this.salesItemNo = salesItemNo;
		this.salesItemCode = salesItemCode;
		this.salesItemType = salesItemType;
		this.salesItemCount = salesItemCount;
		this.salesItemWeight = salesItemWeight;
		this.salesItemPurity = salesItemPurity;
		this.salesItemWastage = salesItemWastage;
		this.salesItemPure = salesItemPure;
		this.salesItemMode = salesItemMode;
		this.salesItemTime = salesItemTime;
	}

	public JewellerySalesModel(String salesBillNo, String salesMetalMno, String salesMetalWeight, String salesMetalResult,
		String salesMetalPure, String salesMetalMode, String salesMetalTime) {
	super();
	this.salesBillNo = salesBillNo;
	this.salesMetalMno = salesMetalMno;
	this.salesMetalWeight = salesMetalWeight;
	this.salesMetalResult = salesMetalResult;
	this.salesMetalPure = salesMetalPure;
	this.salesMetalMode = salesMetalMode;
	this.salesMetalTime = salesMetalTime;
}

	public String getSalesBillNo() {
		return salesBillNo;
	}

	public void setSalesBillNo(String salesBillNo) {
		this.salesBillNo = salesBillNo;
	}

	public String getSalesBillDate() {
		return salesBillDate;
	}

	public void setSalesBillDate(String salesBillDate) {
		this.salesBillDate = salesBillDate;
	}

	public String getSalesBillMobile() {
		return salesBillMobile;
	}

