		this.salesBillMode = salesBillMode;
	}

	public String getSalesBillTime() {
		return salesBillTime;
	}

	public void setSalesBillTime(String salesBillTime) {
		this.salesBillTime = salesBillTime;
	}

	public String getSalesItemNo() {
		return salesItemNo;
	}

	public void setSalesItemNo(String salesItemNo) {
		this.salesItemNo = salesItemNo;
	}

	public String getSalesItemCode() {
		return salesItemCode;
	}

	public void setSalesItemCode(String salesItemCode) {
		this.salesItemCode = salesItemCode;
	}

	public String getSalesItemType() {
		return salesItemType;
	}

	public void setSalesItemType(String salesItemType) {
		this.salesItemType = salesItemType;
	}

	public String getSalesItemWeight() {
		return salesItemWeight;
	}

	public void setSalesItemWeight(String salesItemWeight) {
		this.salesItemWeight = salesItemWeight;
	}

	public String getSalesItemCount() {
		return salesItemCount;
	}

	public void setSalesItemCount(String salesItemCount) {
		this.salesItemCount = salesItemCount;
	}

	public String getSalesItemPurity() {
		return salesItemPurity;
	}

	public void setSalesItemPurity(String salesItemPurity) {
		this.salesItemPurity = salesItemPurity;
	}

	public String getSalesItemWastage() {
		return salesItemWastage; 
	}

	public void setSalesItemWastage(String salesItemWastage) {
		this.salesItemWastage = salesItemWastage;
	}

	public String getSalesItemPure() {
		return salesItemPure;
	}

	public void setSalesItemPure(String salesItemPure) {
		this.salesItemPure = salesItemPure;
	}

	public String getSalesItemMode() {
		return salesItemMode;
	}

	public void setSalesItemMode(String salesItemMode) {
		this.salesItemMode = salesItemMode;
	}

	public String getSalesItemTime() {
		return salesItemTime;
	}

	public void setSalesItemTime(String salesItemTime) {
		this.salesItemTime = salesItemTime;
	}

	public String getSalesMetalWeight() {
		return salesMetalWeight;
	}

	public void setSalesMetalWeight(String salesMetalWeight) {
		this.salesMetalWeight = salesMetalWeight;
	}

	public String getSalesMetalResult() {
		return salesMetalResult;
	}

	public void setSalesMetalResult(String salesMetalResult) {
		this.salesMetalResult = salesMetalResult;
	}

	public String getSalesMetalPure() {
		return salesMetalPure;
	}

	public void setSalesMetalPure(String salesMetalPure) {
		this.salesMetalPure = salesMetalPure;
	}

	public String getSalesBillDateFrom() {
		return salesBillDateFrom;
	}

	public void setSalesBillDateFrom(String salesBillDateFrom) {
		this.salesBillDateFrom = salesBillDateFrom;
	}

	public String getSalesBillDateTo() {
		return salesBillDateTo;
	}

	public void setSalesBillDateTo(String salesBillDateTo) {
		this.salesBillDateTo = salesBillDateTo;
	}

	public String getSalesMetalMno() {
		return salesMetalMno;
	}

	public void setSalesMetalMno(String salesMetalMno) {
		this.salesMetalMno = salesMetalMno;
	}

	public String getSalesMetalMode() {
		return salesMetalMode;
	}

	public void setSalesMetalMode(String salesMetalMode) {
		this.salesMetalMode = salesMetalMode;
	}

	public String getSalesMetalTime() {
